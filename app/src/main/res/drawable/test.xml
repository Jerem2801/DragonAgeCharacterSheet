<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="ring"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content">


    <corners
        android:radius="20dp"/>


    <padding
        android:left="16dp"
        android:right="16dp"
        android:top="10dp"
        android:bottom="10dp"/>

    <stroke
        android:width="1dp"
        android:color="@color/colorPrimary"/>
</shape>
